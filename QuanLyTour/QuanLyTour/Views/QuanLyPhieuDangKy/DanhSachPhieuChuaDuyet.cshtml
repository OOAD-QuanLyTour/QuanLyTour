@model IEnumerable<QuanLyTour.Models.PhieuDangKy>

@{
    ViewBag.Title = "DanhSachPhieuChuaDuyet";
}

<h2>DanhSachPhieuChuaDuyet</h2>
@if (Model != null)
{

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MaKhach)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ngaylap)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TinhTrang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaLichtrinh)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MaKhach)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ngaylap)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TinhTrang)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaLichtrinh)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.MaPhieuDK }) |
                    @Html.ActionLink("Details", "Details", new { id = item.MaPhieuDK }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.MaPhieuDK })
                </td>
            </tr>
        }

    </table>

}
else
{
    <div>
        <p>
            Không có dữ liệu
        </p>
    </div>
}